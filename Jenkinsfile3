pipeline {
    agent none
    stages {
        stage('build1') {
            parallel {
                stage('On Master') {
                    agent {
                        label "master"
                    }
                    steps {
                       bat "mvn clean package"
                    }
                    
                }
                stage(' On Slave_1') {
                    agent {
                        label "Slave_1"
                    }
                    steps {
                       bat "mvn clean package"
                    }
                   
                }
            }
        }
    }
}
